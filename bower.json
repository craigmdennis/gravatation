{
  "name": "jquery.gravatation",
  "version": "1.1.1",
  "homepage": "https://github.com/craigmdennis/gravatation",
  "authors": [
    "Craig Dennis <craigmdennis@gmail.com>"
  ],
  "description": "A jQuery plugin to validate email addresses and retrieve associated Gravatar images.",
  "main": "dist/jquery.gravatation.bundle.js",
  "keywords": [
    "jquery",
    "gravatar",
