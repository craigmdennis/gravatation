    "email"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "blueimp-md5": "~1.1.0",
    "jquery": "~2.1.3",
    "debounce": "https://github.com/cowboy/jquery-throttle-debounce.git"
  },
  "overrides": {
    "jquery": {
      "ignore": "true"
    },
    "debounce": {
      "main": "jquery.ba-throttle-debounce.js"
    }
  }
}
